{
  "name": "TeleBot Panel Pro",
  "short_name": "TeleBot Pro",
  "description": "Advanced Telegram Bot Control Panel with User Bot Management, Secure Terminal, and Real-time Monitoring",
  "version": "2.0.0",
  "manifest_version": 3,
  "icons": [
    {
      "src": "/assets/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/assets/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "start_url": "/",
  "display": "standalone",
  "theme_color": "#0f172a",
  "background_color": "#0f172a",
  "orientation": "any",
  "categories": ["productivity", "utilities"],
  "lang": "en",
  "dir": "ltr",
  "scope": "/",
  "prefer_related_applications": false,
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "Open dashboard",
      "url": "/#dashboard",
      "icons": [{ "src": "/assets/icon-96.png", "sizes": "96x96" }]
    },
    {
      "name": "Bot Control",
      "short_name": "Bots",
      "description": "Manage Telegram bots",
      "url": "/#bot-control",
      "icons": [{ "src": "/assets/icon-96.png", "sizes": "96x96" }]
    },
    {
      "name": "Terminal",
      "short_name": "Terminal",
      "description": "Open secure terminal",
      "url": "/#terminal",
      "icons": [{ "src": "/assets/icon-96.png", "sizes": "96x96" }]
    }
  ]
}
